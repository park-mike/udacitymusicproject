<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicprojectudacity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".songNameActivity"
            android:label="List of Song Names" />
        <activity
            android:name=".PlaylistActivity"
            android:label="Playlists" />
        <activity
            android:name=".MusicStoreActivity"
            android:label="Music Store" />
        <activity
            android:name=".SongSearch"
            android:label="Search for Songs" />

    </application>

</manifest>